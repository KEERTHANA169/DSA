class Solution {
    public int majorityElement(int[] nums) {
        HashMap<Integer,Integer> map=new HashMap<>();

        for(int num:nums)
        {
            map.put(num,map.getOrDefault(num,0)+1);
        }

        int maxfreq=0;
        int maxval=-1;

        for(int key:map.keySet())
        {
            int freq =map.get(key);
            if(freq>maxfreq)
            {
                maxfreq=freq;
                maxval=key;
            }
        }
        return maxval;
    }
}
